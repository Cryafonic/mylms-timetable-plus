{
   "name": "MyLMS Timetable Plus",
   "description": "Enhance your MyLMS Timetable.",
   "author": "Kagiso Marvin Molekwa",
   "version": "0.9.0",
   "manifest_version": 3,
   "permissions": [
      "activeTab",
      "scripting"
   ],
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "matches": ["https://mylms.vossie.net/mytimetable/*"],
         "js": ["content.js"]
      }
   ]
 }